{
    "name": "szemul/queue",
    "description": "Queue handler interfaces",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.210.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.4.6",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\Queue\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
